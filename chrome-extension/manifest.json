{

  "name": "HBL Hack",
  "version": "0.1",
  "description": "Get more sleep.",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    },
    {
      "matches": [
        "https://adfs.ri.edu.sg/adfs/*"
      ],
      "js": ["autoclick.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }

}
